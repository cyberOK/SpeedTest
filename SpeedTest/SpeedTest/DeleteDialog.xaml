<ContentDialog 
    
    x:Uid="DeleteDialog"
    x:Class="SpeedTestUWP.DeleteDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:SpeedTestUWP"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d" 
    PrimaryButtonStyle="{StaticResource ContentDialogPrimaryButtonStyle}"
    Title="{Binding Localizator[DeleteDialogTitle], Source={StaticResource mainViewModel}}" 
    Content="{Binding Localizator[DeleteDialogContent], Source={StaticResource mainViewModel}}"
    PrimaryButtonText="{Binding Localizator[DeleteDialogPrimaryButtonText], Source={StaticResource mainViewModel}}" 
    SecondaryButtonText="{Binding Localizator[DeleteDialogSecondaryButtonText], Source={StaticResource mainViewModel}}"     
    RequestedTheme="{Binding SettingsPanel.Settings.Theme, Source={StaticResource mainViewModel}}"
    PrimaryButtonCommand="{Binding DeleteHistoryContentDialogButtonPressed, Source={StaticResource mainViewModel}}">

</ContentDialog>
